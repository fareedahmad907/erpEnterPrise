<%= render '/shared/page_title', title: "login" %>
<div class="row">
	<div class=" col col-lg-4 col-md-offset-4">
		<%= form_tag '/login' do %>
		<%= label_tag :email  %>
		<%= text_field_tag :email %>
		
		<%= label_tag :password  %>
		<%= password_field_tag :password %>
		
		<%= submit_tag 'LOGIN' ,class:"btn btn-success"  %>
		<% end %>
	</div>
</div>
